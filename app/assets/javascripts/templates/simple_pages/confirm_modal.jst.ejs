<div id="confirm-modal" class="modal simple-modal">
  <div class="modal-dialog animated">
    <div class="modal-content">
      <div class="modal-body">
        <p><%= content %></p>
      </div>
      <div class="modal-footer">
        <button class="btn" role='submit-modal'><%= btnTitle %></button>
        <button class="btn btn-gray" role='close-modal'><%= cancelTitle %></button>
      </div>
    </div>
  </div>
</div>